{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": ["@react-native-community", "prettier"],
  "plugins": ["@typescript-eslint", "react", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "jest": true
  }
}