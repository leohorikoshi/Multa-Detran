{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Expo: Run on Web",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["expo", "start", "--web"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Expo: Debug",
      "type": "reactnative",
      "request": "launch",
      "platform": "expolocal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Expo: Run on Android",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}"
    }
  ]
}
